{
  "env": {
    "es2021": true,
    "node": true,
    "mocha": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-param-reassign": "off",
    "import/prefer-default-export": "off",
    "no-underscore-dangle": "off"
  }
}
